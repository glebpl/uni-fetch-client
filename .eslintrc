{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": [
    "import"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors"
  ],
  "rules": {
    "semi": 2,
    "prefer-arrow-callback": 2,
    "quotes": [1, "double"],
    "no-unused-vars": 1,
    "no-console": 0
  }
}
